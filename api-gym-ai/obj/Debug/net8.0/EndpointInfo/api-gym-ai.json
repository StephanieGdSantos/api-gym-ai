{
  "openapi": "3.0.1",
  "info": {
    "title": "api-gym-ai",
    "version": "1.0"
  },
  "paths": {
    "/Exercicio": {
      "post": {
        "tags": [
          "Exercicio"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Usuario"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Usuario"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Usuario"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Exercicio"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Exercicio"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Exercicio"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Exercicio": {
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "nullable": true
          },
          "series": {
            "type": "integer",
            "format": "int32"
          },
          "repeticoes": {
            "type": "string",
            "nullable": true
          },
          "musculoAlvo": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "InfoCorporais": {
        "required": [
          "altura",
          "idade",
          "peso"
        ],
        "type": "object",
        "properties": {
          "idade": {
            "maximum": 100,
            "minimum": 10,
            "type": "integer",
            "format": "int32"
          },
          "altura": {
            "maximum": 2,8,
            "minimum": 1,3,
            "type": "number",
            "format": "double"
          },
          "peso": {
            "maximum": 500,
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "percentualGordura": {
            "maximum": 50,
            "minimum": 10,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "massaMuscular": {
            "maximum": 50,
            "minimum": 15,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "sexo": {
            "type": "string",
            "nullable": true
          },
          "limitacoes": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "InfoObjetivo": {
        "required": [
          "objetivo",
          "partesDoCorpoEmFoco",
          "tempoDeTreino",
          "variacaoTreino"
        ],
        "type": "object",
        "properties": {
          "objetivo": {
            "minLength": 1,
            "type": "string"
          },
          "partesDoCorpoEmFoco": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "tempoDeTreino": {
            "maximum": 240,
            "minimum": 20,
            "type": "number",
            "format": "double"
          },
          "variacaoTreino": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Usuario": {
        "type": "object",
        "properties": {
          "infoCorporais": {
            "$ref": "#/components/schemas/InfoCorporais"
          },
          "objetivo": {
            "$ref": "#/components/schemas/InfoObjetivo"
          }
        },
        "additionalProperties": false
      }
    }
  }
}