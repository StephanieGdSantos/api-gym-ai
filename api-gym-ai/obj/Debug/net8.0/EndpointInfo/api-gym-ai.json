{
  "openapi": "3.0.1",
  "info": {
    "title": "api-gym-ai",
    "version": "1.0"
  },
  "paths": {
    "/PersonalTrainer": {
      "post": {
        "tags": [
          "PersonalTrainer"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InformacoesCorporais"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/InformacoesCorporais"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/InformacoesCorporais"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Exercicio"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Exercicio"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Exercicio"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/WeatherForecast": {
      "get": {
        "tags": [
          "WeatherForecast"
        ],
        "operationId": "GetWeatherForecast",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Exercicio": {
        "required": [
          "musculoAlvo",
          "nome",
          "repeticoes",
          "series"
        ],
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "nullable": true
          },
          "series": {
            "type": "integer",
            "format": "int32"
          },
          "repeticoes": {
            "type": "string",
            "nullable": true
          },
          "musculoAlvo": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "InformacoesCorporais": {
        "required": [
          "altura",
          "idade",
          "limitacoes",
          "objetivo",
          "partesDoCorpoEmFoco",
          "peso",
          "tempoDeTreino"
        ],
        "type": "object",
        "properties": {
          "idade": {
            "type": "integer",
            "format": "int32"
          },
          "altura": {
            "type": "number",
            "format": "double"
          },
          "peso": {
            "type": "number",
            "format": "double"
          },
          "percentualGordura": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "massaMuscular": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "sexo": {
            "type": "string",
            "nullable": true
          },
          "limitacoes": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "objetivo": {
            "type": "string",
            "nullable": true
          },
          "partesDoCorpoEmFoco": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "tempoDeTreino": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "summary": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}